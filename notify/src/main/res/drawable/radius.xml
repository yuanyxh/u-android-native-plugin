<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--阴影-->
    <item
        android:left="0.12dp"
        android:top="0.12dp"
        android:bottom="0.12dp"
        android:right="0.12dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="#02000000" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item
        android:left="0.11dp"
        android:top="0.11dp"
        android:bottom="0.11dp"
        android:right="0.11dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="#03000000" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item
        android:left="0.1dp"
        android:top="0.1dp"
        android:bottom="0.1dp"
        android:right="0.1dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="#04000000" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <!--前景-->
    <item
        android:left="2dp"
        android:top="2dp"
        android:bottom="2dp"
        android:right="2dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="5dp" />
        </shape>
    </item>
</layer-list>